<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>SakanatCom</title>
    <link rel="icon" type="image/x-icon" href="~/Admin/assets/sacan/images/favicon.svg" />
    <!-- Bootstrap -->
    <link href="~/Admin/assets/sacan/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Main styles -->
    <link href="~/Admin/assets/sacan/css/style.css" rel="stylesheet" />
    <!--<link rel="stylesheet/less" href="css/style.less" />-->
    <!-- IE styles -->
    <link href="~/Admin/assets/sacan/css/ie.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Admin/assets/sacan/css/font-awesome.min.css" rel="stylesheet" />
    <!-- OWL Carousel -->
    <link href="~/Admin/assets/sacan/css/owl.carousel.css" rel="stylesheet" />
    <!-- Jquery UI -->
    <link href="~/Admin/assets/sacan/css/jquery-ui.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
   
</head>

<body>
    <!-- Overlay preloader-->
    <div class="htlfndr-loader-overlay"></div>

    <div class="htlfndr-wrapper">
        <header>
            <div class="htlfndr-top-header">
                <div class="navbar navbar-default htlfndr-blue-hover-nav">
                    <div class="container">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button"
                                    class="navbar-toggle collapsed"
                                    data-toggle="collapse"
                                    data-target="#htlfndr-first-nav">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <div class="htlfndr-logo navbar-brand" >
                                <img src="~/Admin/assets/sacan/images/logo.svg" alt="Logo" style="top: 7px" />
                                <p class="htlfndr-logo-text">Sakanat<span>Com</span></p>
                            </div>
                        </div>
                        <!-- .navbar-header -->
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse navbar-right" id="htlfndr-first-nav">
                            <!-- List with sing up/sing in links -->
                            <ul class="nav navbar-nav htlfndr-singup-block">
                                <li style="margin-top: 4%; color: #23def7;" id="htlfndr-singup-link">
                                   <span>@User.Identity.Name</span>
                                </li>
                                <li id="htlfndr-singin-link">
                                    <a asp-controller="Account" asp-action="Logout">
                                        <span>Sign In/Sign Out</span>
                                    </a>
                                </li>
                            </ul>
                            <!-- .htlfndr-singup-block -->
                            <!-- .htlfndr-top-menu-dropdowns -->
                        </div>
                        <!-- .collapse.navbar-collapse -->
                    </div>
                    <!-- .container -->
                </div>
                <!-- .navbar.navbar-default.htlfndr-blue-hover-nav-->
            </div>

            <div class="htlfndr-under-header">
                <nav class="navbar navbar-default">
                    <div class="container">
                        <div class="navbar-header">
                            <button type="button"
                                    class="navbar-toggle collapsed"
                                    data-toggle="collapse"
                                    data-target="#htlfndr-main-nav">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <!-- .navbar-header -->
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="htlfndr-main-nav">
                            <ul class="nav navbar-nav">
                                <li>
                                    <a asp-action="Index" asp-controller="Home">Home</a>
                                </li>
                                <li>
                                    <a asp-action="Index" asp-controller="About">About Us</a>
                                </li>

                                <li>
                                    <a asp-action="Index" asp-controller="Prop">Discover Properties</a>
                                </li>

                               
                                <li>
                                    @if (User.Identity.IsAuthenticated)
                                    {
                                        string userEmail = User.Identity.Name.ToLower();
                                        if (!userEmail.Contains("@std"))
                                        {
                                            <a asp-action="Index" asp-controller="Contact">Contact Us</a>
                                        }
                                    }
                                    else
                                    {
                                        <a asp-action="Index" asp-controller="Contact">Contact Us</a>
                                    }
                                </li>
                         @if (User.Identity.IsAuthenticated)
        {
            string userEmail = User.Identity.Name.ToLower();
            if (userEmail.Contains("@hotmail") ||
                userEmail.Contains("@gmail") ||
                userEmail.Contains("@icloud") ||
                userEmail.Contains("@outlook") ||
                userEmail.Contains("@yahoo"))
            {
                <li>
                    <a asp-action="Index" asp-controller="List">Add/Edit/Delete Your Properties</a>
                </li>
            }
        }
        @if (User.Identity.IsAuthenticated && User.Identity.Name.Contains("kareemmaher2002@icloud.com"))
        {
                                <li>
                                    <a asp-action="Index" asp-controller="AdminS" >Admin Panel</a>
                                </li>
        }

                            </ul>
                        </div>
                        <!-- .collapse.navbar-collapse -->
                    </div>
                    <!-- .container -->
                </nav>
                <!-- .navbar.navbar-default.htlfndr-blue-hover-nav -->
            </div>
</header>
            </div>
            @RenderBody()


	<footer class="htlfndr-footer">
		<button class="htlfndr-button-to-top" role="button">
			<span>Back to top</span>
		</button><!-- Button "To top" -->

		<div class="widget-wrapper">
			<div class="container">
				<div class="row">
             
					<aside class="col-xs-12 col-sm-6 col-md-3 htlfndr-widget-column">
						<div class="widget">
							<a class="htlfndr-logo navbar-brand" href="#">
                                <img src="~/Admin/assets/sacan/images/logo.svg"
									 height="20"
									 width="30"
									 alt="Logo" />
								<p class="htlfndr-logo-text">Sakanat<span>Com</span></p>
							</a>
							<hr />
							<p>
								Your Gateway to Ideal Student Accommodation. We specialize
								in connecting students with the perfect accommodation that
								suits their needs and preferences.
							</p>
							<br />
							<p>
								find your home away from home with ease. Simplify your
								student life today with Sakanatcom.
							</p>
						</div>
						<!-- .widget -->
					</aside>
					<!-- .col-xs-12.col-sm-6.col-md-3.htlfndr-widget-column -->
					<aside class="col-xs-12 col-sm-6 col-md-3 htlfndr-widget-column">
						<div class="widget">
							<h3 class="widget-title">contact info</h3>
							<h5>address</h5>
							<p>SakanatCom <br />120 ZRQ - Hashemite Uni 214, JOR</p>
							<hr />
							<h5>phone number</h5>
							<p>078-888-888-8</p>
							<hr />
							<h5>email address</h5>
							<p>support@SakanatCom.Hashemite.com</p>
						</div>
						<!-- .widget -->
					</aside>
					<!-- .col-xs-12.col-sm-6.col-md-3.htlfndr-widget-column -->
                    <aside class="col-xs-12 col-sm-6 col-md-3 htlfndr-widget-column">
                        <div class="widget">
                            
                        </div>
                        <!-- .widget -->
                    </aside>
					<!-- .col-xs-12.col-sm-6.col-md-3.htlfndr-widget-column -->
					<aside class="col-xs-12 col-sm-6 col-md-3 htlfndr-widget-column">
						<div class="widget">
                            <br />
                            <br />
                            <br />
                            <br />
                            <br />
                            <br />
                            <br />
                            <br />
							<h3 class="widget-title">follow us</h3>
							<!-- Start of Follow Us buttons -->
							<div class="htlfndr-follow-plugin">
								<a href="https://www.facebook.com"
								   target="_blank"
								   class="htlfndr-follow-button button-facebook"></a>
								<a href="https://twitter.com"
								   target="_blank"
								   class="htlfndr-follow-button button-twitter"></a>
								<a href="https://plus.google.com"
								   target="_blank"
								   class="htlfndr-follow-button button-google-plus"></a>
								<a href="https://www.linkedin.com"
								   target="_blank"
								   class="htlfndr-follow-button button-linkedin"></a>
								<a href="#"
								   class="htlfndr-follow-button button-pinterest"></a>
								<a href="https://www.youtube.com"
								   target="_blank"
								   class="htlfndr-follow-button button-youtube"></a>
							</div>
					
						</div>
						<!-- .widget -->
					</aside>
                  
					<!-- .col-xs-12.col-sm-6.col-md-3.htlfndr-widget-column -->
				</div>
				<!-- .row -->
			</div>
			<!-- .container -->
		</div>
		<!-- .widget-wrapper -->

		<div class="htlfndr-copyright">
			<div class="container" role="contentinfo">
				<p>
					Copyright 2023 | Hashemite universty | All Rights Reserved |
					Designed by SWE team 2023
				</p>
			</div>
			<!-- .container -->
		</div>
		<!-- .htlfndr-copyright -->
	</footer>





            <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
            <script src="~/Admin/assets/sacan/js/jquery-1.11.3.min.js"></script>
            <!-- Include Jquery UI script file -->
            <script src="~/Admin/assets/sacan/js/jquery-ui.min.js"></script>
            <!-- Include all compiled plugins (below), or include individual files as needed -->
            <!-- Include Query UI Touch Punch is a small hack that enables the use
             of touch events on sites using the jQuery UI user interface library. -->
            <script src="~/Admin/assets/sacan/js/jquery.ui.touch-punch.min.js"></script>
            <script src="~/Admin/assets/sacan/js/bootstrap.min.js"></script>
            <!-- Include Owl Carousel script file -->
            <script src="~/Admin/assets/sacan/js/owl.carousel.min.js"></script>
            <!-- Include main script file -->
            <script src="~/Admin/assets/sacan/js/script.js"></script>

            
           
</body>
</html>


